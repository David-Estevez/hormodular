<ECF>
	<Algorithm>
		<DifferentialEvolution>
			<Entry key="F">1</Entry>                  <!-- scaling constant -->
			<Entry key="CR">0.9</Entry>               <!-- crossover rate -->
		</DifferentialEvolution>
	</Algorithm>
	
    <Genotype>
    
   		<FloatingPoint>			<!-- Genotype for the amplitudes -->
			<Entry key="lbound">0</Entry>
			<Entry key="ubound">90</Entry>
			<Entry key="dimension">3</Entry>
		</FloatingPoint>
    	
		<FloatingPoint>			<!-- Genotype for the offsets -->
			<Entry key="lbound">-90</Entry>
			<Entry key="ubound">90</Entry>
			<Entry key="dimension">3</Entry>
		</FloatingPoint>
		
		<FloatingPoint>			<!-- Genotype for the phases -->
			<Entry key="lbound">0</Entry>
			<Entry key="ubound">360</Entry>
			<Entry key="dimension">3</Entry>
		</FloatingPoint>
		
    </Genotype>
    
    <Registry>
    	<!-- population -->
		<Entry key="population.size">10</Entry> <!-- number of individuals (default: 100) -->
		<Entry key="migration.freq">10</Entry> <!-- individuals are exchanged each 'freq' generations (default: none) -->
		<Entry key="migration.number">2</Entry> <!-- number of individuals to be sent to another deme (default: 1) -->
	
    	<!-- Termination conditions -->
    	<Entry key="term.maxgen">50</Entry> 	<!-- max number of generations (default: none) -->	
    	
		<!-- logging and checkpointing -->
		<Entry key="log.level">5</Entry> <!-- log level; valid values are 1 (minimal) to 5 (verbose) (default: 3) -->
		<Entry key="log.filename">log.txt</Entry> <!-- log filename (default: none) -->
		<Entry key="log.frequency">5</Entry> <!-- log only every 'frequency' generations (default: 1) -->
		<Entry key="log.level">5</Entry> <!-- log level; valid values are 1 (minimal) to 5 (verbose) (default: 3) -->
		<Entry key="milestone.filename">out.txt</Entry> <!-- milestone file (if stated) stores all the population (default: none) -->
		<Entry key="milestone.interval">5</Entry> <!-- milestone saving interval in generations; 0: save only at the end (default: 0) -->
    	
    	<!-- Robot custom entries -->
    	<Entry key='robot.modules'>3</Entry> <!-- Number of modules -->
    	<Entry key='robot.runtime'>10000</Entry> <!-- Max runtime (ms) -->
    	<Entry key='robot.timestep'>1</Entry> <!-- Time step (ms) -->
    	<Entry key='robot.simulationfile'>../../data/models/Minicube-II.env.xml</Entry>  <!-- Simulation model file -->
    	<Entry key='robot.gaittablefile'>../../data/gait tables/evolving_gaits.txt</Entry> <!-- Gait table file -->
    </Registry>
</ECF>
