<Environment>

  <physicsengine type="ode">
    <odeproperties>
      <friction>0.1</friction>
      <gravity>0 0 -9.81</gravity>
      <selfcollision>1</selfcollision>
    </odeproperties>
  </physicsengine>

  <Robot name="Unimod">
    <kinbody prefix="m1_" file = "Unimod.kinbody.xml">
      <translation> 0 0 0.036</translation>
    </kinbody>
  </Robot>

  <KinBody name="floor">
    <Body type="static">
      <Translation>0 0 -0.003</Translation>
      <Geom type="box">
        <extents>0.3 0.3 0.003 </extents>
        <diffuseColor>.3 1 .3</diffuseColor>
      	<ambientColor>0.3 1 0.3</ambientColor>
        <transparency>0.2</transparency>
      </Geom>
    </Body>
  </KinBody>


</Environment>
