<!-- Head2P is two head segments attached back to back
-->

<Kinbody name="khead2Y">
   <Body name="Head2Y" type="dynamic">
      <mass type="sphere">
        <total>0.025</total>
        <radius>0.018</radius>
      </mass>
      <Geom type="trimesh">
        <Render>Y1-head.iv 0.001</Render>
        <data>Y1-head.iv 0.001</data>
        <rotationaxis>0 1 0 -90</rotationaxis>
        <translation>0 0.018 -0.007</translation>
      </Geom>
      <Geom type="trimesh">
        <Render>Y1-head.iv 0.001</Render>
        <data>Y1-head.iv 0.001</data>
        <rotationaxis>0 0 1 180</rotationaxis>
        <rotationaxis>0 1 0 90</rotationaxis>
        <translation>0 0.0540 -0.007</translation>
      </Geom>
    </Body>
</Kinbody>

