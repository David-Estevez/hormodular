<Environment>

  <physicsengine type="ode">
    <odeproperties>
      <friction>0.2</friction>
      <gravity>0 0 -9.81</gravity>
      <selfcollision>1</selfcollision>
    </odeproperties>
  </physicsengine>

  <Robot file = "Minicube-I-flat-2.robot.xml">
    <translation>0 0.0 0.00001</translation>
  </Robot>

   <KinBody name="floor">
    <Body type="static">
      <Translation>0 0 -0.003</Translation>
      <Geom type="box">
        <extents>0.1 0.5 0.003 </extents>
        <diffuseColor>.3 1 .3</diffuseColor>
      	<ambientColor>0.3 1 0.3</ambientColor>
        <transparency>0.2</transparency>
      </Geom>
    </Body>
  </KinBody>


</Environment>

